<li class="dropdown nav-item">
    <a asp-controller="Lanche" asp-action="List" class="dropdown-toggle text-dark nav-link" data-toggle="dropdown">Lanches</a>
    <ul class="dropdown-menu">
        @foreach (var categoria in Model)
        {
            <li>
                <a class="dropdown-item" asp-controller="Lanche" asp-action="List" style="color:black"
                   asp-route-categoria="@categoria.Nome">@categoria.Nome</a>
            </li>
        }
        <li class="dropdown-divider"></li>
        <li>
            <a class="dropdown-item" asp-controller="Lanche" asp-action="List" asp-route-categoria="" style="color:black">Todos os Lanches</a>
        </li>
    </ul>
</li>